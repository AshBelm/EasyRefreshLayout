<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EasyRefreshLayout">
        <attr name="headOverlap" format="boolean"/>
        <attr name="footOverlap" format="boolean"/>
        <attr name="header" format="reference"/>
        <attr name="footer" format="reference"/>
        <attr name="headMaxDistance" format="dimension"/>
        <attr name="headMinDistance" format="dimension"/>
        <attr name="headActivateDistance" format="dimension"/>
        <attr name="footMaxDistance" format="dimension"/>
        <attr name="footMinDistance" format="dimension"/>
        <attr name="footActivateDistance" format="dimension"/>
        <attr name="headerDock" format="enum">
            <enum name="before" value="0"/>
            <enum name="after" value="1"/>
        </attr>
        <attr name="footerDock" format="enum">
            <enum name="before" value="0"/>
            <enum name="after" value="1"/>
        </attr>
        <attr name="footerSpringFixed" format="boolean"/>
    </declare-styleable>
</resources>